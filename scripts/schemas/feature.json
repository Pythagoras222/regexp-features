{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "Feature",
    "type": "object",
    "properties": {
        "feature": { "type": "string" },
        "name": { "type": "string" },
        "priority": { "type": "number" },
        "aliases": { "type": "array", "items": { "type": "string" } },
        "description": { "type": "string" },
        "syntax": { "type": "string" },
        "example": { "type": "string" },
        "see_also": {
            "oneOf": [
                { "type": "string" },
                { "type": "array", "items": { "$ref": "Link" } }
            ]
        },
        "links": { "type": "array", "items": { "$ref": "HrefLinkDefinition" } },
        "content": { "type": "string" }
    },
    "required": ["feature"]
}